{
  "name": "ellma",
  "version": "0.1.4",
  "description": "Easy LLM Assistants",
  "license": "MIT",
  "author": "David Myers <hello@davidmyers.dev>",
  "type": "module",
  "funding": "https://github.com/sponsors/davidmyersdev",
  "homepage": "https://github.com/davidmyersdev/ellma",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidmyersdev/ellma.git"
  },
  "bugs": {
    "url": "https://github.com/davidmyersdev/ellma/issues"
  },
  "files": [
    "dist",
    "dist/**/*",
    "integrations/dist",
    "integrations/dist/**/*",
    "integrations/package.json",
    "models/dist",
    "models/dist/**/*",
    "models/package.json",
    "peripherals/dist",
    "peripherals/dist/**/*",
    "peripherals/package.json"
  ],
  "keywords": [
    "chat",
    "chatgpt",
    "gpt",
    "langchain",
    "language-models",
    "llm",
    "openai",
    "terminal",
    "tty"
  ],
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./integrations": {
      "types": "./integrations/dist/index.d.ts",
      "import": "./integrations/dist/index.js",
      "require": "./integrations/dist/index.cjs"
    },
    "./models": {
      "types": "./models/dist/index.d.ts",
      "import": "./models/dist/index.js",
      "require": "./models/dist/index.cjs"
    },
    "./peripherals": {
      "types": "./peripherals/dist/index.d.ts",
      "import": "./peripherals/dist/index.js",
      "require": "./peripherals/dist/index.cjs"
    }
  },
  "scripts": {
    "build": "run-s clean && run-p build:* && run-s types",
    "build:core": "vite build",
    "build:integrations": "vite build ./integrations",
    "build:peripherals": "vite build ./peripherals",
    "clean": "run-p clean:*",
    "clean:core": "rimraf ./dist ./types",
    "clean:integrations": "rimraf ./integrations/dist ./integrations/types",
    "clean:peripherals": "rimraf ./peripherals/dist ./peripherals/types",
    "ellma": "vite-node --script ./ellma.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "types": "tsc && run-p types:*",
    "types:core": "rollup -c ./types.config.js",
    "types:integrations": "rollup -c ./integrations/types.config.js",
    "types:peripherals": "rollup -c ./peripherals/types.config.js"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "cross-fetch": "^3.1.5",
    "events": "^3.3.0",
    "langchain": "^0.0.77",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "openai": "^3.2.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@types/events": "^3.0.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.1",
    "cac": "^6.7.14",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-artisan": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.3",
    "rollup-plugin-dts": "^5.3.0",
    "type-hype": "^0.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-node": "^0.30.1",
    "vite-plugin-externalize-deps": "^0.7.0"
  }
}
