{
  "name": "ellma",
  "type": "module",
  "version": "0.1.1",
  "description": "Easy LLM Agents",
  "author": "David Myers <hello@davidmyers.dev>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/davidmyersdev",
  "homepage": "https://github.com/davidmyersdev/ellma",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidmyersdev/ellma.git"
  },
  "bugs": {
    "url": "https://github.com/davidmyersdev/ellma/issues"
  },
  "files": [
    "dist",
    "dist/**/*"
  ],
  "keywords": [
    "chat",
    "chatgpt",
    "gpt",
    "openai",
    "terminal",
    "tty"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "ellma": "vite-node --script ./ellma.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "cross-fetch": "^3.1.5",
    "events": "^3.3.0",
    "langchain": "^0.0.77",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "openai": "^3.2.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.1",
    "cac": "^6.7.14",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-artisan": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "type-hype": "^0.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-node": "^0.30.1",
    "vite-plugin-externalize-deps": "^0.6.0"
  },
  "imports": {
    "#data": "./src/data/index.ts",
    "#data/internal": "./src/data/internal/index.ts",
    "#data/utils": "./src/data/utils.ts",
    "#data/*": "./src/data/*",
    "#globals": "./src/globals/index.ts",
    "#integrations": "./src/integrations/index.ts",
    "#integrations/openai": "./src/integrations/openai/index.ts",
    "#integrations/*": "./src/integrations/*",
    "#models": "./src/models/index.ts",
    "#models/chat": "./src/models/chat/index.ts",
    "#models/*": "./src/models/*",
    "#peripherals": "./src/peripherals/index.ts",
    "#peripherals/http": "./src/peripherals/http/index.ts",
    "#peripherals/storage": "./src/peripherals/storage/index.ts",
    "#peripherals/*": "./src/peripherals/*"
  }
}
